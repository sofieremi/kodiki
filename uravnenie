# ax^2 + bx + c = 0
x1 = float(input('enter x1'))
x2 = float(input('enter x2'))
b = x2 + x1
c = x1 * x2
if b < 0 and c < 0:
    print('x^2', b, 'x', c, ' = 0')
elif b < 0 and c > 0:
    print('x^2', b, 'x +', c, ' = 0')
elif b > 0 and c < 0:
    print('x^2 +', b, 'x', c, ' = 0')
else:
    print('x^2 +', b, 'x +', c, ' = 0')
